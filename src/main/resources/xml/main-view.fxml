<?xml version="1.0" encoding="UTF-8"?>

<?import com.tech.fx.Content?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefWidth="1080.0"
	stylesheets="@../css/main-view.css" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.tech.main.MainController">
	<children>
		<GridPane layoutX="121.0" layoutY="103.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="NEVER" />
				<RowConstraints vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<AnchorPane>
					<children>
						<fx:include fx:id="toolBar" source="tool-bar.fxml"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
				<AnchorPane GridPane.rowIndex="1">
					<children>
						<Content fx:id="content" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
			</children>
		</GridPane>
	</children>
</AnchorPane>
